{
  "name": "xproxy",
  "version": "0.1.2",
  "description": "http/https proxy server",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require should --reporter spec",
    "test-cov": "istanbul cover ./node_modules/.bin/_mocha -- --require should",
    "test-travis": "istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- --require should",
    "start": "node-dev index.js server",
    "server": "pm2 delete xproxy & pm2 start index.js server --watch --name=\"xproxy\" & sudo npm run logs",
    "stop": "pm2 stop xproxy",
    "kill": "pm2 delete xproxy",
    "logs": "pm2 logs xproxy",
    "debug": "DEBUG=*,-koa-send node_modules/.bin/node-dev index.js -p 80 -s 443",
    "demo": "node_modules/.bin/node-dev server.js",
    "autod": "autod"
  },
  "bin": {
    "xproxy": "./bin/xproxy"
  },
  "keywords": [
    "proxy",
    "http",
    "https",
    "server"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/daizch/xproxy.git"
  },
  "author": "zechengdai",
  "license": "MIT",
  "dependencies": {
    "bytes": "3.0.0",
    "chalk": "2.3.0",
    "cheerio": "1.0.0-rc.2",
    "co": "4.6.0",
    "co-urllib": "0.2.3",
    "commander": "2.11.0",
    "debug": "3.1.0",
    "expand-home-dir": "0.0.3",
    "fs-extra": "4.0.2",
    "http-proxy": "1.16.2",
    "humanize-number": "0.0.2",
    "ip": "1.1.5",
    "koa": "2.4.1",
    "koa-static": "4.0.1",
    "lodash": "4.17.4",
    "mime": "2.0.3",
    "passthrough-counter": "1.0.0",
    "semver": "5.4.1",
    "shelljs": "0.7.8"
  ,
    "weinre": "^2.0.0-pre-I0Z7U9OV"
  },
  "devDependencies": {
    "autod": "^3.0.0",
    "co-request": "^1.0.0",
    "iconv-lite": "^0.4.15",
    "istanbul": "^0.4.3",
    "mocha": "^2.5.3",
    "pm2": "^1.1.3",
    "should": "^9.0.0",
    "supertest": "^1.2.0"
  },
  "directories": {
    "test": "test"
  }
}
